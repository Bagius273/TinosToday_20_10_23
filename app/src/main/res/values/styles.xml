<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize primary color -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <!-- Customize darker variant of primary color -->
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <!-- Customize accent color -->
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="CircularButton" parent="Widget.MaterialComponents.Button">
        <item name="android:background">@drawable/circular_button_background</item>
        <item name="backgroundTint">@null</item> <!-- Disable background tint -->
    </style>


</resources>